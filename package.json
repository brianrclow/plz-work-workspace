    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^11.0.0",
    "electron": "^12.2.3",
    "electron-builder": "^22.10.0",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~15.2.0",
    "electron-rebuild": "~2.3.4",
