{
  "name": "plz-work-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "clean": "npx rimraf hooks node_modules package-lock.json yarn.lock && yarn",
    "postinstall": "electron-rebuild install-app-deps",
    "build.electron.plz-electron": "npm run prepare.electron.plz-electron && ng build electron-plz-electron --prod --base-href ./",
    "build.electron.plz-electron.local": "npm run build.electron.plz-electron && electron dist/apps/electron-plz-electron",
    "build.electron.plz-electron.linux": "npm run build.electron.plz-electron && cd dist/apps/electron-plz-electron && npx electron-builder build --linux",
    "build.electron.plz-electron.windows": "npm run build.electron.plz-electron && cd dist/apps/electron-plz-electron && npx electron-builder build --windows",
    "build.electron.plz-electron.mac": "npm run build.electron.plz-electron && cd dist/apps/electron-plz-electron && npx electron-builder build --mac",
    "prepare.electron.plz-electron": "tsc -p apps/electron-plz-electron/tsconfig.json",
    "serve.electron.plz-electron.target": "nx serve electron-plz-electron",
    "serve.electron.plz-electron": "wait-on http-get://localhost:4200/ && electron apps/electron-plz-electron/src --serve",
    "start.electron.plz-electron": "npm run prepare.electron.plz-electron && npm-run-all -p serve.electron.plz-electron.target serve.electron.plz-electron"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@nativescript/eslint-plugin": "~0.0.4",
    "@nativescript/types": "~8.4.0",
    "@nativescript/webpack": "~5.0.12",
    "@ngtools/webpack": "^15.0.0",
    "@nrwl/angular": "^15.0.0",
    "@nrwl/cli": "15.2.4",
    "@nrwl/cypress": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/jest": "15.2.4",
    "@nrwl/linter": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@nstudio/angular": "15.0.2",
    "@nstudio/electron": "15.0.2",
    "@nstudio/electron-angular": "15.0.2",
    "@nstudio/nativescript": "^15.0.0",
